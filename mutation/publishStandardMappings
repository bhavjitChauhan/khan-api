mutation publishStandardMappings($set: String!, $content: String!) {
  publishStandardMappings(setId: $set, contentDescriptor: $content) {
    published {
      id
      setId
      standardId
      contentDescriptor
      secondaryDescriptor
      deletion: isDeletion
      __typename
    }
    error {
      code
      __typename
    }
    __typename
  }
}