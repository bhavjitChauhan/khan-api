mutation createStripeSession($priceID: String!, $paymentFrequency: PaymentFrequency!, $cancelURL: String!, $successURL: String!) {
  createStripeSession(
    priceID: $priceID
    paymentFrequency: $paymentFrequency
    cancelURL: $cancelURL
    successURL: $successURL
  ) {
    redirectURL
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}