mutation loginWithCleverMutation($code: String!, $reauth: Boolean!, $fromDevApp: Boolean!, $isMap: Boolean!, $isLP: Boolean!, $isCanvas: Boolean, $referrerUrl: String!) {
  loginWithClever(
    code: $code
    reauth: $reauth
    fromDevApp: $fromDevApp
    isMap: $isMap
    isLP: $isLP
    isCanvas: $isCanvas
    referrerUrl: $referrerUrl
  ) {
    error {
      code
      cleverUserInfo {
        cleverId
        isTeacher
        cleverEmail
        __typename
      }
      transferToken
      udiUuid
      __typename
    }
    user {
      id
      kaid
      canAccessDistrictsHomepage
      isTeacher
      hasUnresolvedInvitations
      preferredKaLocale {
        id
        kaLocale
        status
        __typename
      }
      __typename
    }
    isLibraryUser
    isFirstLogin
    transferToken
    __typename
  }
}