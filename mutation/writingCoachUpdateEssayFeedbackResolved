mutation writingCoachUpdateEssayFeedbackResolved($essaySessionID: String!, $feedbackID: String!, $isResolved: Boolean!) {
  updateEssayFeedbackResolved(
    essaySessionID: $essaySessionID
    feedbackID: $feedbackID
    isResolved: $isResolved
  ) {
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}