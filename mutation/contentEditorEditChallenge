mutation contentEditorEditChallenge($input: EditChallengeInput!, $sha: String!) {
  editChallenge(input: $input, sha: $sha) {
    challenge {
      id
      contentId
      contentKind
      listed
      title
      description
      code
      sha
      doNotPublish
      tests
      __typename
    }
    error {
      code
      debugMessage
      conflictSha
      __typename
    }
    __typename
  }
}