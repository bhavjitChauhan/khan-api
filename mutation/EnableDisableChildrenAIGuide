mutation EnableDisableChildrenAIGuide($enableKAIDs: [String!]!, $disableKAIDs: [String!]!) {
  aiGuideDeactivateChildAccounts(childKAIDs: $disableKAIDs) {
    errors {
      code
      debugMessage
      kaid
      __typename
    }
    __typename
  }
  aiGuideActivateChildAccounts(childKAIDs: $enableKAIDs) {
    errors {
      code
      debugMessage
      kaid
      __typename
    }
    __typename
  }
}