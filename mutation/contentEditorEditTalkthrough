mutation contentEditorEditTalkthrough($sha: String!, $input: EditTalkthroughInput!) {
  editTalkthrough(sha: $sha, input: $input) {
    talkthrough {
      ...TalkthroughRevision
      __typename
    }
    error {
      code
      conflictSha
      debugMessage
      __typename
    }
    __typename
  }
}

fragment TalkthroughRevision on TalkthroughRevision {
  id
  contentId
  contentKind
  slug
  sha
  listed
  __typename
}
