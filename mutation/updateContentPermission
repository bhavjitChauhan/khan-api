mutation updateContentPermission($userKaid: String!, $id: String!, $role: String!, $kaLocale: String!, $curationNodeId: String!, $kind: String!, $canPublish: Boolean!) {
  updateContentPermission(
    userKaid: $userKaid
    contentPermission: {id: $id, role: $role, kaLocale: $kaLocale, curationNodeId: $curationNodeId, kind: $kind, canPublish: $canPublish}
  ) {
    error {
      code
      __typename
    }
    __typename
  }
}