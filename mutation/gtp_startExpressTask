mutation gtp_startExpressTask($taskType: String!, $taskId: ID!) {
  startTask(taskType: $taskType, taskId: $taskId) {
    descriptorList {
      id
      checkpointStr
      ...gtp_expressDescriptorsFragment
      __typename
    }
    practiceTest {
      ...gtp_practiceTestFragment
      __typename
    }
    task {
      ...gtp_taskFragment
      __typename
    }
    tpud {
      id
      diagnosticsStates {
        type
        state
        __typename
      }
      hasSeededSkillLevels
      __typename
    }
    __typename
  }
}