mutation saveLatestEssaySession($essaySessionID: String!, $essayInstructions: String!, $studentGradeLevel: Int!, $essayText: String!, $essayTitle: String!, $essayVersion: Int!, $paragraphIDToParagraphNumbers: [ParagraphIDToParagraphNumbersInputEntry!]!) {
  saveLatestEssayText(
    essaySessionID: $essaySessionID
    essayInstructions: $essayInstructions
    studentGradeLevel: $studentGradeLevel
    essayText: $essayText
    essayTitle: $essayTitle
    essayVersion: $essayVersion
    paragraphIDToParagraphNumbers: $paragraphIDToParagraphNumbers
    stage: REVISING
  ) {
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}