mutation saveLatestEssaySession($essaySessionID: String!, $essayInstructions: String!, $studentGradeLevel: Int!, $essayText: String!) {
  saveLatestEssayText(
    essaySessionID: $essaySessionID
    essayInstructions: $essayInstructions
    studentGradeLevel: $studentGradeLevel
    essayText: $essayText
  ) {
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}