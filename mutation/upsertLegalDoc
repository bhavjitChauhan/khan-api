mutation upsertLegalDoc($legalDocument: LegalDocumentInput!, $notes: String!) {
  upsertLegalDocument(legalDocument: $legalDocument, notes: $notes) {
    legalDocument {
      id
      name
      slug
      isArchived
      kaLocales
      countryCodes
      draftContent
      __typename
    }
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}