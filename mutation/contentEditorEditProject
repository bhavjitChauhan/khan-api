mutation contentEditorEditProject($sha: String!, $input: EditProjectInput!) {
  editProject(sha: $sha, input: $input) {
    project {
      ...ProjectRevision
      __typename
    }
    error {
      code
      conflictSha
      debugMessage
      __typename
    }
    __typename
  }
}

fragment ProjectRevision on ProjectRevision {
  id
  contentId
  contentKind
  sha
  doNotPublish
  slug
  listed
  title
  description
  projectEval
  authorName
  code
  codeFormat
  customTitleTag
  customDescriptionTag
  sourceKaLocale
  __typename
}