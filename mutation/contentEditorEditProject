mutation contentEditorEditProject($sha: String!, $input: EditProjectInput!) {
  editProject(sha: $sha, input: $input) {
    project {
      ...ProjectRevision
      __typename
    }
    error {
      code
      conflictSha
      debugMessage
      __typename
    }
    __typename
  }
}

fragment ProjectRevision on ProjectRevision {
  id
  contentId
  contentKind
  slug
  description
  listed
  title
  sha
  code
  doNotPublish
  projectEval
  __typename
}