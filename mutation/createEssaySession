mutation createEssaySession($essayText: String!, $essayInstructions: String!, $studentGradeLevel: Int!) {
  createEssaySession(
    essayInstructions: $essayInstructions
    essayText: $essayText
    studentGradeLevel: $studentGradeLevel
  ) {
    essaySessionID
    error {
      debugMessage
      __typename
    }
    __typename
  }
}