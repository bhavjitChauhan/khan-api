mutation LearnPrototype_PracticeTask($exerciseId: String!, $assignmentKey: String) {
  getOrCreatePracticeTask(
    input: {exerciseId: $exerciseId, assignmentKey: $assignmentKey, stopCardPersist: false, canReserveItems: true}
  ) {
    result {
      error {
        code
        debugMessage
        __typename
      }
      userTask {
        task {
          ...practiceTaskFields
          __typename
        }
        userExercises {
          ...practiceTaskUserExerciseFields
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}

fragment practiceTaskExerciseFields on Exercise {
  id
  contentKind
  slug
  topicPaths {
    path {
      id
      kind
      __typename
    }
    __typename
  }
  problemTypeKind
  numAssessmentItems
  translatedDisplayName
  relatedContent {
    ...relatedContentFields
    __typename
  }
  __typename
}

fragment practiceTaskFields on PracticeTask {
  id
  key
  exerciseLength
  taskType
  contentKey
  reservedItems
  reservedItemsCompleted
  taskAttemptHistory {
    correct
    timeDone
    seenHint
    itemId
    __typename
  }
  __typename
}

fragment practiceTaskUserExerciseFields on UserExercise {
  exercise {
    ...practiceTaskExerciseFields
    __typename
  }
  fpmMasteryLevel
  lastAttemptNumber
  lastCountHints
  __typename
}

fragment relatedContentFields on LearnableContent {
  id
  contentKind
  thumbnailUrl
  defaultUrlPath
  translatedTitle
  ... on Video {
    duration
    translatedYoutubeId
    __typename
  }
  __typename
}