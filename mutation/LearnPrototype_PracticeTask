mutation LearnPrototype_PracticeTask($exerciseId: String!, $assignmentKey: String) {
  getOrCreatePracticeTask(
    input: {exerciseId: $exerciseId, assignmentKey: $assignmentKey, stopCardPersist: false, canReserveItems: true}
  ) {
    result {
      error {
        code
        debugMessage
        __typename
      }
      userTask {
        task {
          ...practiceTaskFields
          __typename
        }
        userExercises {
          ...exerciseTaskUserExerciseEntityFields
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}

fragment exerciseTaskAttemptHistoryFields on TaskProblemAttempt {
  correct
  timeDone
  seenHint
  itemId
  __typename
}

fragment exerciseTaskExerciseFields on Exercise {
  id
  contentKind
  slug
  topicPaths {
    path {
      id
      kind
      __typename
    }
    __typename
  }
  problemTypeKind
  numAssessmentItems
  translatedDisplayName
  relatedContent {
    ...relatedContentFields
    __typename
  }
  __typename
}

fragment exerciseTaskUserExerciseEntityFields on UserExercise {
  exercise {
    ...exerciseTaskExerciseFields
    __typename
  }
  fpmMasteryLevel
  lastAttemptNumber
  lastCountHints
  __typename
}

fragment practiceTaskFields on PracticeTask {
  id
  key
  exerciseLength
  taskType
  contentKey
  reservedItems
  reservedItemsCompleted
  isCompleted
  taskAttemptHistory {
    ...exerciseTaskAttemptHistoryFields
    __typename
  }
  __typename
}

fragment relatedContentFields on LearnableContent {
  id
  contentKind
  thumbnailUrl
  defaultUrlPath
  translatedTitle
  ... on Video {
    duration
    translatedYoutubeId
    __typename
  }
  __typename
}