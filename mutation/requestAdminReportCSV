mutation requestAdminReportCSV($kaid: ID!, $selectedNodeID: ID!, $childIDs: [String!]!, $reportType: AdminReportType!, $startDate: DateTime!, $endDate: DateTime!, $grades: [String!], $teacherKaids: [String!], $courseSISValues: [String!], $courseID: String, $strandKey: String, $bands: [String!], $notifyByEmail: Boolean!, $kaLocale: String) {
  requestAdminReportCSV(
    kaid: $kaid
    selectedNodeID: $selectedNodeID
    childIDs: $childIDs
    reportType: $reportType
    startDate: $startDate
    endDate: $endDate
    teacherKaids: $teacherKaids
    grades: $grades
    courseSISValues: $courseSISValues
    courseID: $courseID
    strandKey: $strandKey
    bands: $bands
    notifyByEmail: $notifyByEmail
    kaLocale: $kaLocale
  ) {
    adminReport {
      id
      status
      __typename
    }
    error {
      code
      __typename
    }
    __typename
  }
}