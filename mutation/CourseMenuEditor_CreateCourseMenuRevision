mutation CourseMenuEditor_CreateCourseMenuRevision($newCourseMenu: NewCourseMenuInput!) {
  createCourseMenu(input: $newCourseMenu) {
    courseMenu {
      ...CourseMenuRevisionFields
      __typename
    }
    error {
      code
      __typename
    }
    __typename
  }
}

fragment CourseMenuRevisionFields on CourseMenuRevision {
  id
  sha
  groups {
    identifier
    translatedTitle
    href
    isNew
    children {
      identifier
      translatedTitle
      href
      isNew
      nonContentLink
      loggedOutHref
      __typename
    }
    __typename
  }
  __typename
}