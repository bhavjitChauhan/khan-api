mutation coeditingAutosave($configName: String!, $threadId: String!, $title: String!, $prefix: String!, $selection: String!, $suffix: String!, $persona: AIGuidePersona!) {
  saveCoeditingSnapshot(
    configName: $configName
    threadId: $threadId
    title: $title
    prefix: $prefix
    selection: $selection
    suffix: $suffix
    persona: $persona
  ) {
    error {
      code
      __typename
    }
    __typename
  }
}