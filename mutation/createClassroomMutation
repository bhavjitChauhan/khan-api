mutation createClassroomMutation($classroomName: String!, $subscribeToUpdates: Boolean, $isTestClassroom: Boolean, $isChildAssignmentsClassroom: Boolean) {
  createClassroom(
    classroomName: $classroomName
    subscribeToUpdates: $subscribeToUpdates
    isTestClassroom: $isTestClassroom
    isChildAssignmentsClassroom: $isChildAssignmentsClassroom
  ) {
    classroom {
      id
      cacheId
      descriptor
      key
      name
      signupCode
      isChildAssignmentsClassroom
      __typename
    }
    error {
      code
      __typename
    }
    __typename
  }
}