mutation createClassroomMutation($classroomName: String!, $subscribeToUpdates: Boolean, $isTestClassroom: Boolean) {
  createClassroom(
    classroomName: $classroomName
    subscribeToUpdates: $subscribeToUpdates
    isTestClassroom: $isTestClassroom
  ) {
    classroom {
      id
      cacheId
      descriptor
      key
      name
      signupCode
      __typename
    }
    error {
      code
      __typename
    }
    __typename
  }
}