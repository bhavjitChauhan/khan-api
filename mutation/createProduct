mutation createProduct($input: MutateProductInput!) {
  createProduct(input: $input) {
    product {
      id
      name
      isActive
      isRecurring
      description
      extCampaignID
      extProductID
      classyValidationAmount
      validationInterval
      validationBillingCountries
      __typename
    }
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}
