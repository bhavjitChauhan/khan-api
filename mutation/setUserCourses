mutation setUserCourses($courseIds: [String]!, $gradeLevelId: String) {
  setUserCourses(courseIds: $courseIds, gradeLevelId: $gradeLevelId) {
    error {
      code
      __typename
    }
    user {
      id
      homepageModules {
        navigation {
          interestedInLsat
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}