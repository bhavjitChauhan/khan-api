mutation loginWithPasswordAndTwoFactorCodeMutation($identifier: String!, $password: String!, $twoFactorAuthCode: String!) {
  loginWithPassword(
    identifier: $identifier
    password: $password
    twoFactorAuthCode: $twoFactorAuthCode
  ) {
    user {
      id
      kaid
      canAccessDistrictsHomepage
      isTeacher
      hasUnresolvedInvitations
      preferredKaLocale {
        id
        kaLocale
        status
        __typename
      }
      __typename
    }
    isFirstLogin
    error {
      code
      __typename
    }
    __typename
  }
}