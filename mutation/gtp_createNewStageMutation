mutation gtp_createNewStageMutation($examId: String!, $stageIndex: Int!) {
  createNewStage(examId: $examId, stageIndex: $stageIndex) {
    checkpoints {
      ...gtp_checkpointFragment
      __typename
    }
    tpud {
      id
      currentStage
      currentCheckpoint
      __typename
    }
    __typename
  }
}