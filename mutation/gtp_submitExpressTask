mutation gtp_submitExpressTask($taskId: ID!, $taskType: String!, $newTaskState: JSONString!, $newTaskStateHash: ID!, $prevTaskStateHash: ID) {
  submitTask(
    taskId: $taskId
    taskType: $taskType
    newTaskState: $newTaskState
    newTaskStateHash: $newTaskStateHash
    prevTaskStateHash: $prevTaskStateHash
  ) {
    checkpoints {
      ...gtp_checkpointFragment
      __typename
    }
    descriptorList {
      id
      checkpointStr
      ...gtp_expressDescriptorsFragment
      __typename
    }
    practiceTest {
      ...gtp_practiceTestFragment
      __typename
    }
    task {
      ...gtp_taskFragment
      __typename
    }
    tpud {
      ...gtp_tpudFragment
      __typename
    }
    egud {
      ...gtp_egudFragment
      __typename
    }
    __typename
  }
}