mutation createChildMutation($birthdate: Date!, $username: String!, $password: String!, $nickname: String) {
  createChild(
    birthdate: $birthdate
    username: $username
    password: $password
    nickname: $nickname
  ) {
    child {
      id
      nickname
      __typename
    }
    error {
      code
      field
      __typename
    }
    __typename
  }
}