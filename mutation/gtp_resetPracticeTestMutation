mutation gtp_resetPracticeTestMutation($examId: String!, $taskId: String!) {
  resetPracticeTest(examId: $examId, taskId: $taskId) {
    error {
      code
      debugMessage
      __typename
    }
    practiceTests {
      ...gtp_practiceTestFragment
      __typename
    }
    tpud {
      ...gtp_tpudFragment
      __typename
    }
    __typename
  }
}