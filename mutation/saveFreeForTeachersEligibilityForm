mutation saveFreeForTeachersEligibilityForm($teacherRoleOp: RoleOperation!, $birthdate: String!) {
  setSettings(birthdate: $birthdate) {
    errors {
      code
      __typename
    }
    __typename
  }
  updateUserRole(role: TEACHER, operation: $teacherRoleOp) {
    error {
      code
      __typename
    }
    __typename
  }
}