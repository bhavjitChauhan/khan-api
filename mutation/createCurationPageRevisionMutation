mutation createCurationPageRevisionMutation($input: NewCurationPageInput!) {
  createCurationPage(input: $input) {
    curationPage {
      contentKind
      id
      title
      slug
      alternateSlugs
      description
      listed
      sponsored
      showCurationModule
      lowerToc
      customDescriptionTag
      customTitleTag
      curationData
      sha
      hasUnpublishedChanges
      __typename
    }
    error {
      code
      conflictSha
      debugMessage
      __typename
    }
    __typename
  }
}
