mutation csEditorEditProject($input: EditProjectInput!, $sha: String!) {
  editProject(input: $input, sha: $sha) {
    project {
      ...ProjectRevision
      __typename
    }
    error {
      code
      conflictSha
      debugMessage
      __typename
    }
    __typename
  }
}

fragment ProjectRevision on ProjectRevision {
  id
  contentId
  contentKind
  slug
  description
  listed
  title
  sha
  code
  doNotPublish
  projectEval
  __typename
}