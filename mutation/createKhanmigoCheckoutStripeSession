mutation createKhanmigoCheckoutStripeSession($priceID: String!, $paymentFrequency: PaymentFrequency!, $cancelURL: String!, $successURL: String!, $couponID: String) {
  createStripeSession(
    priceID: $priceID
    paymentFrequency: $paymentFrequency
    cancelURL: $cancelURL
    successURL: $successURL
    couponID: $couponID
  ) {
    redirectURL
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}