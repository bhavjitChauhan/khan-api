mutation SetGuidePreferences($readingLevel: AIGuideReadingLevel, $guideLanguageCode: String, $guideVoiceDescriptor: String, $showDABs: Boolean) {
  setPreferencesForUser(
    readingLevel: $readingLevel
    guideLanguageCode: $guideLanguageCode
    guideVoiceDescriptor: $guideVoiceDescriptor
    showDABs: $showDABs
  ) {
    error {
      code
      __typename
    }
    __typename
  }
}