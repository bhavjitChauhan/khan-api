mutation addDistrictCourseProgressDateRange($selectedNodeID: String!, $dateRange: CustomDateRange!) {
  addDistrictCourseProgressDateRange(
    selectedNodeID: $selectedNodeID
    dateRange: $dateRange
  ) {
    dateRanges {
      from
      isCustom
      isInProgress
      lastUpdatedDate
      numDays
      rangeType
      upTo
      __typename
    }
    error {
      code
      __typename
    }
    __typename
  }
}