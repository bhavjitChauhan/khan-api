mutation gtp_startTask($taskType: String!, $taskId: ID!) {
  startTask(taskType: $taskType, taskId: $taskId) {
    descriptorList {
      id
      checkpointStr
      ...gtp_descriptorsFragment
      __typename
    }
    practiceTest {
      ...gtp_practiceTestFragment
      __typename
    }
    task {
      ...gtp_taskFragment
      __typename
    }
    tpud {
      id
      hasSeededSkillLevels
      __typename
    }
    __typename
  }
}