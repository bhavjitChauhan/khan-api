mutation KAClassroom_CreateAssignment($courseSlug: String!, $classDescriptors: [String!]!, $qualifiedContentDescriptorsList: [String!], $startDate: DateTime!, $dueDate: DateTime, $isDraft: Boolean, $studentKaids: [ID], $questionSetType: ExerciseItemPickerStrategy) {
  createAssignments(
    assignment: {courseSlug: $courseSlug, classroomDescriptors: $classDescriptors, qualifiedContentDescriptorsList: $qualifiedContentDescriptorsList, startDate: $startDate, dueDate: $dueDate, isDraft: $isDraft, studentKaids: $studentKaids, exerciseItemPickerStrategy: $questionSetType}
  ) {
    assignments {
      id
      classroom {
        id
        cacheId
        descriptor
        __typename
      }
      __typename
    }
    __typename
  }
}