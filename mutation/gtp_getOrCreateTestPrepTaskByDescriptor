mutation gtp_getOrCreateTestPrepTaskByDescriptor($examId: String!, $taskType: String!, $exerciseName: String!, $skillId: String, $level: String) {
  getOrCreateTestPrepTaskByDescriptor(
    examId: $examId
    taskType: $taskType
    exerciseName: $exerciseName
    skillId: $skillId
    level: $level
  ) {
    task {
      ...gtp_taskFragment
      __typename
    }
    practiceTest {
      ...gtp_practiceTestFragment
      __typename
    }
    __typename
  }
}