mutation SetCleverInfo($cleverDistrictId: String!, $cleverId: String!, $kaid: String!) {
  setCleverInfoAsAdmin(
    cleverDistrictId: $cleverDistrictId
    cleverId: $cleverId
    kaid: $kaid
  ) {
    error {
      code
      __typename
    }
    user {
      id
      cleverId
      cleverDistrictId
      __typename
    }
    __typename
  }
}