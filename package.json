  },
  "packageManager": "yarn@3.3.1",
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.2.6",
    "@types/minimist": "^1",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.3.1",
    "minimist": "^1.2.7",
    "prettier": "2.8.3",
    "rimraf": "^4.1.1",
    "rollup": "^3.10.0",
    "rollup-plugin-dts": "^5.1.1",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.24",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./esm/khan-api.js",
