{
  "name": "@bhavjit/khan-api",
  "author": "Bhavjit Chauhan",
  "keywords": [
    "khan-academy"
  ],
  "packageManager": "yarn@3.3.1",
  "version": "0.2.0",
  "main": "./dist/node/khan-api.cjs",
  "exports": {
    ".": {
      "browser": "./dist/khan-api.js",
      "default": {
        "require": "./dist/node/khan-api.cjs",
        "default": "./dist/esm/khan-api.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier ./**/*.ts --write",
    "types": "tsc --emitDeclarationOnly && rollup -c && rimraf dist/dts",
    "build": "yarn node ./scripts/build.js",
    "doc": "typedoc"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.15",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "rollup-plugin-dts": "^5.1.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.24",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.4"
  }
}
