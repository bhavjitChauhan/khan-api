{
  "name": "@bhavjit/khan-api",
  "version": "0.5.3",
  "description": "A Khan Academy API client and wrapper",
  "keywords": [
    "khan-academy",
    "api"
  ],
  "homepage": "https://khan-api.bhavjit.com",
  "bugs": "https://github.com/bhavjitChauhan/khan-api/issues",
  "license": "MIT",
  "author": "Bhavjit Chauhan",
  "files": [
    "esm/",
    "node/",
    "example.js",
    "index.d.ts",
    "khan-api.js",
    "khan-api.js.map",
    "khan-api.min.js",
    "khan-api.min.js.map"
  ],
  "module": "./esm/khan-api.js",
  "main": "./node/khan-api.cjs",
  "browser": "./khan-api.js",
  "types": "./index.d.ts",
  "repository": "github:bhavjitChauhan/khan-api",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write ./**/*.ts ./esm/*.js ./node/*.cjs khan-api.js",
    "clean": "rimraf esm node index.d.ts khan-api.js khan-api.js.map khan-api.min.js khan-api.min.js.map",
    "types": "tsc --emitDeclarationOnly && rollup -c && rimraf dist/types",
    "build": "yarn node ./scripts/build.js",
    "doc": "typedoc"
  },
  "packageManager": "yarn@3.5.0",
  "type": "module",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.15.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.12",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "minimist": "^1.2.8",
    "prettier": "2.8.5",
    "rimraf": "^4.4.0",
    "rollup": "^3.20.0",
    "rollup-plugin-dts": "^5.3.0",
    "ts-jest": "^29.0.5",
    "typedoc": "^0.23.28",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "import": "./esm/khan-api.js",
    "require": "./node/khan-api.cjs",
    "browser": "./khan-api.js",
    "default": "./esm/khan-api.js"
  },
  "runkitExampleFilename": "example.js"
}
