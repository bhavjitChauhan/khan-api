{
  "name": "@bhavjit/khan-api",
  "version": "0.2.0",
  "description": "A Khan Academy API client and wrapper",
  "keywords": [
    "khan-academy",
    "api"
  ],
  "homepage": "https://bhavjitchauhan.github.io/khan-api",
  "bugs": "https://github.com/bhavjitChauhan/khan-api/issues",
