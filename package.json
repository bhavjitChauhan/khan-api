    "node/",
    "example.js",
    "index.d.ts",
    "khan-api.js",
    "khan-api.js.map",
    "khan-api.min.js",
    "khan-api.min.js.map"
  ],
  "module": "./esm/khan-api.js",
  "main": "./node/khan-api.cjs",
