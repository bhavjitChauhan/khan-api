{
  "name": "@bhavjit/khan-api",
  "author": "Bhavjit Chauhan",
  "version": "0.1.0",
  "keywords": [
    "khan-academy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bhavjitChauhan/khan-api.git"
  },
  "bugs": {
    "url": "https://github.com/bhavjitChauhan/khan-api/issues"
  },
  "homepage": "https://bhavjitchauhan.github.io/khan-api",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "esm/",
    "node/",
    "index.d.ts",
    "khan-api.js",
    "khan-api.js.map",
    "khan-api.min.js",
    "khan-api.min.js.map"
  ],
  "exports": {
    ".": {
      "browser": "./khan-api.js",
      "default": {
        "require": "./node/khan-api.cjs",
        "default": "./esm/khan-api.js"
      }
    }
  },
  "main": "./khan-api.js",
  "types": "./index.d.ts",
  "runkitExampleFilename": "example.js",
  "packageManager": "yarn@3.3.1",
  "type": "module",
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier ./**/*.ts --write",
    "types": "tsc --emitDeclarationOnly && rollup -c && rimraf dist/types",
    "build": "yarn node ./scripts/build.js",
    "doc": "typedoc"
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "chalk": "^5.2.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.15",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "jest": "^29.3.1",
    "prettier": "2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1",
    "rollup-plugin-dts": "^5.1.1",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.24",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.4"
  }
