query getTeacherAccessPermissionsData {
  user {
    id
    age
    joined
    isTeacher
    isFormalTeacher
    isK4dTeacher
    hasAccessToAIGuideTeacher
    isAIGuideEnabled
    hasClasses
    isKmapStudent
    isK4dStudent
    hasAccessToAIGuideDistrictClassroomTeacher
    classrooms {
      id
      cacheId
      isK4dClassroom
      __typename
    }
    __typename
  }
}