query getTeacherAccessPermissionsData {
  user {
    id
    age
    joined
    isTeacher
    isFormalTeacher
    isK4dTeacher
    hasAccessToAIGuideTeacher
    isAIGuideEnabled
    hasChildren
    hasClasses
    isKmapStudent
    isK4dStudent
    hasAccessToAIGuideDistrictClassroomTeacher
    classrooms {
      id
      cacheId
      isK4dClassroom
      __typename
    }
    coachedChildAssignmentsClassrooms {
      id
      cacheId
      students {
        id
        includesDistrictOwnedData
        __typename
      }
      __typename
    }
    __typename
  }
}