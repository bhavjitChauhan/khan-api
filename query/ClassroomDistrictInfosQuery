query ClassroomDistrictInfosQuery($kaid: String) {
  coach: user(kaid: $kaid) {
    id
    classroomDistrictInfos: coachedClassroomDistrictInfos(primaryTeacherOnly: true) {
      id
      studentList {
        id
        classDescriptor: descriptor
        cacheId
        name
        signupCode
        topics {
          id
          slug
          translatedStandaloneTitle
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}
