query SkillsToProficient_LearnerRecommendations($courseId: String, $classroomDescriptor: String, $numRecs: Int) {
  user {
    id
    recommendations(
      queryParams: {courseId: $courseId, classroomDescriptor: $classroomDescriptor, numRecs: $numRecs}
    ) {
      content {
        id
        defaultUrlPath
        __typename
      }
      __typename
    }
    __typename
  }
}