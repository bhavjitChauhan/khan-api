query GetNotationReadAloudsForAssessmentItem($assessmentItemID: ID!, $exerciseID: ID!) {
  getNotationReadAloudsForAssessmentItem(
    assessmentItemID: $assessmentItemID
    exerciseID: $exerciseID
  ) {
    notationReadAlouds {
      assessmentItemId
      domainId
      exerciseId
      exerciseUrl
      id
      readable
      original
      __typename
    }
    error {
      code
      message
      __typename
    }
    __typename
  }
}