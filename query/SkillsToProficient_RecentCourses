query SkillsToProficient_RecentCourses($pageSize: Int!) {
  user {
    id
    recentCourses(pageSize: $pageSize) {
      results {
        content {
          id
          isListedForLearners
          masteryEnabled
          relativeUrl
          translatedTitle
          ... on Course {
            masterableExercises(includeDuplicates: true) {
              id
              __typename
            }
            __typename
          }
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}