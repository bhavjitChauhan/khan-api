query CourseMenuEditor_DefaultCourseMenu($curriculum: String!) {
  defaultCourseMenu: learnMenuTopics(curriculum: $curriculum) {
    identifier: slug
    translatedTitle
    href
    isNew
    children {
      identifier: slug
      translatedTitle
      href
      isNew
      loggedOutHref
      nonContentLink
      __typename
    }
    __typename
  }
}
