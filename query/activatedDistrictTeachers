query activatedDistrictTeachers($districtName: String!, $pageSize: Int, $cursor: Int) {
  activationReportByTeacher: activationReportByTeacherV2(
    districtName: $districtName
    pageSize: $pageSize
    cursor: $cursor
  ) {
    rows {
      isActivated
      teacherKaid
      teacherName
      __typename
    }
    __typename
  }
}