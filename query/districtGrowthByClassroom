query districtGrowthByClassroom($teacherKaid: String!, $schoolID: String!, $filters: DistrictGrowthFilters!) {
  districtGrowthByClassroom(
    teacherKaid: $teacherKaid
    schoolID: $schoolID
    filters: $filters
  ) {
    classroom {
      id
      cacheId
      descriptor
      name
      coachKaid
      classroomDistrictInfo {
        id
        name
        __typename
      }
      __typename
    }
    info {
      numStudents
      medianCMPercent
      wowPercentChange
      eoyPercentGuess
      targetStatus
      __typename
    }
    __typename
  }
}