query getEotMCCardDetails($taskId: String!, $topicId: String!) {
  user {
    id
    exerciseData {
      masteryChallengeAttempt(taskId: $taskId, topicId: $topicId) {
        id
        numAttempted
        numCorrect
        pointsEarned
        skillsLeveledDown
        skillsLeveledUp
        skillsNoChange
        skillLevelChanges {
          id
          exercise {
            id
            title
            translatedTitle
            __typename
          }
          before
          after
          changeDirection
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}