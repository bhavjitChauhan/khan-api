query getKhanLabsSettings($targetKaid: String!) {
  user(kaid: $targetKaid) {
    id
    age
    isParent
    isTeacher
    isAIGuideEnabled
    aiGuideEnrollmentGroup
    aiGuideDisabledReason
    nickname
    includesDistrictOwnedData
    children {
      id
      age
      nickname
      isAIGuideEnabled
      aiGuideEnrollmentGroup
      aiGuideDisabledReason
      includesDistrictOwnedData
      aiGuideAccessPlans {
        id
        isActive
        enrollmentGroup
        __typename
      }
      __typename
    }
    hasKhanmigoViaStripe
    everHadKhanmigoViaStripe
    aiGuideAccessPlans {
      id
      isActive
      enrollmentGroup
      __typename
    }
    __typename
  }
}