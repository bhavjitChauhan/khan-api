query getKhanLabsSettings($targetKaid: String!) {
  user(kaid: $targetKaid) {
    id
    age
    isParent
    isAIGuideEnabled
    aiGuideDisabledReason
    nickname
    includesDistrictOwnedData
    children {
      id
      age
      nickname
      isAIGuideEnabled
      aiGuideDisabledReason
      includesDistrictOwnedData
      __typename
    }
    hasKhanmigoViaClassy
    hasKhanmigoViaStripe
    __typename
  }
}
