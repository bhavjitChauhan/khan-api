query DistrictActivationByTeacherContainer($districtId: ID!, $schoolIds: [ID!], $teacherIDs: [ID!], $pageSize: Int, $cursor: Int) {
  activationReportByTeacher: activationReportByTeacherV2(
    districtId: $districtId
    schoolIds: $schoolIds
    teacherIDs: $teacherIDs
    pageSize: $pageSize
    cursor: $cursor
  ) {
    rows {
      teacherId
      teacherName
      numStudentsTotal
      numStudentsActivated
      isActivated
      numStudentsStartedAnActivity
      __typename
    }
    totalCount
    updatedAt
    __typename
  }
}