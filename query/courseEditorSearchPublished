query courseEditorSearchPublished($query: String!, $contentKinds: [String]) {
  searchPage(
    query: $query
    contentKinds: $contentKinds
    numResults: 125
    region: "*"
  ) {
    results {
      contentKind: kind
      contentId
      learnableContent {
        id
        title
        translatedTitle
        ... on Exercise {
          interimAssessmentType
          __typename
        }
        __typename
      }
      curationNode {
        id
        title
        translatedTitle
        __typename
      }
      __typename
    }
    __typename
  }
}