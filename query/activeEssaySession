query activeEssaySession {
  activeEssaySession {
    draft {
      feedbackList {
        id
        dimension
        feedbackText
        isPositive
        isResolved
        relevantParagraphId
        relevantQuotedText
        thread {
          id
          interactions {
            answer
            customMessages
            flagged
            id
            question
            __typename
          }
          __typename
        }
        __typename
      }
      id
      submittedText
      __typename
    }
    essayInstructions
    id
    mostRecentEditedText
    essayVersion
    paragraphIDToParagraphNumbers {
      paragraphID
      paragraphNumbers
      __typename
    }
    studentGradeLevel
    __typename
  }
}