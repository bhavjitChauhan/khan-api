query gtp_getEssayScores($examId: String!) {
  essayScores(examId: $examId) {
    ...gtp_essayScoresFragment
    __typename
  }
}

fragment gtp_essayScoresFragment on EssayScores {
  areas {
    translatedTitle
    essays {
      examCompletionDate
      score
      maxScore
      __typename
    }
    __typename
  }
  __typename
}