query translationPortalContentProgress($contentDescriptors: [String!]!, $contentLocale: String!) {
  contentTranslationProgress(
    contentDescriptors: $contentDescriptors
    contentKALocale: $contentLocale
  ) {
    content {
      ...tapAIGuideActivityNode
      ...tapArticleNode
      ...tapChallengeNode
      ...tapExerciseNode
      ...tapInteractiveNode
      ...tapProjectNode
      ...tapTalkthroughNode
      ...tapVideoNode
      __typename
    }
    __typename
  }
}