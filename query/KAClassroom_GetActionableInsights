query KAClassroom_GetActionableInsights($classroomDescriptor: String) {
  actionableInsights(queryBy: {classroomDescriptor: $classroomDescriptor}) {
    id
    type
    callsToAction {
      type
      __typename
    }
    classroom {
      id
      cacheId
      name
      __typename
    }
    __typename
  }
}