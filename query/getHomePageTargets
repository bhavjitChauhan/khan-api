query getHomePageTargets($districtID: ID!) {
  districtById(districtId: $districtID) {
    id
    goal {
      courseMasteryTargets {
        course {
          id
          title: translatedTitle
          __typename
        }
        gradeLevels
        __typename
      }
      khanmigoTarget {
        percentage
        __typename
      }
      learningMinutesTarget {
        percentage
        __typename
      }
      skillsLeveledUp {
        skillsPerWeek
        __typename
      }
      skillsToProficient {
        skillsPerWeek
        __typename
      }
      __typename
    }
    __typename
  }
}