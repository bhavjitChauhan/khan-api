query learnMenuTopicsQuery($region: String) {
  learnMenuTopics(region: $region) {
    domainId
    slug
    translatedTitle
    href
    isNew
    icon
    children {
      courseId
      slug
      translatedTitle
      href
      isNew
      loggedOutHref
      nonContentLink
      __typename
    }
    __typename
  }
}