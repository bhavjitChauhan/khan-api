query getAiGuideThreads($kaid: String!, $hasCoeditor: Boolean!, $limit: Int!) {
  aiGuideThreads(kaid: $kaid, hasCoeditor: $hasCoeditor, limit: $limit) {
    threads {
      id
      coeditingDocument(excludeArchived: true) {
        id
        title
        configName
        threadId
        latestSnapshot {
          id
          createdAt
          __typename
        }
        __typename
      }
      blooketQuestionSet {
        id
        title
        threadId
        lastUpdatedAt
        __typename
      }
      __typename
    }
    __typename
  }
}