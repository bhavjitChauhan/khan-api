query MappedStandardsForContent($contentDescriptor: String!, $locale: String, $region: String) {
  standards: standardMappingsForContent(contentDescriptor: $contentDescriptor, locale: $locale, region: $region) {
    setId
    id
    standardId
    description
    relativeUrl
    setOfStandards {
      id
      isDiscoverable
      __typename
    }
    __typename
  }
}
