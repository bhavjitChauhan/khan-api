query districtKhanmigoTotalUsageByNode($filter: KhanmigoDistrictFilters!) {
  districtKhanmigoUsage(filter: $filter) {
    groupType
    overall {
      possibleUsers
      usersWithUsage
      percentUsersWithUsage
      chats
      avgChatsPerUsersWithUsage
      avgChatsPerPossibleUsers
      flaggedChats
      avgFlaggedChatsPerUsersWithUsage
      avgFlaggedChatsPerPossibleUsers
      messages
      avgMessagesPerChat
      avgMessagesPerUserWithUsage
      avgMessagesPerPossibleUsers
      targetStatus
      __typename
    }
    target {
      percentage
      __typename
    }
    byAdminAggregate {
      adminAggregate {
        ... on District {
          id
          name
          __typename
        }
        ... on MetaDistrict {
          id
          name
          __typename
        }
        __typename
      }
      usage {
        possibleUsers
        usersWithUsage
        percentUsersWithUsage
        chats
        avgChatsPerUsersWithUsage
        avgChatsPerPossibleUsers
        flaggedChats
        avgFlaggedChatsPerUsersWithUsage
        avgFlaggedChatsPerPossibleUsers
        messages
        avgMessagesPerChat
        targetStatus
        avgMessagesPerUserWithUsage
        avgMessagesPerPossibleUsers
        __typename
      }
      __typename
    }
    __typename
  }
}