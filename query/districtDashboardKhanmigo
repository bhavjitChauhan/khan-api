query districtDashboardKhanmigo($filters: DistrictDashboardFilters!, $userType: DistrictUserTypeFilter) {
  districtDashboardKhanmigo(filters: $filters, userType: $userType) {
    lastWeekStart
    lastWeekEnd
    lastWeekUsage {
      usersWithUsage
      percentUsersWithUsage
      avgChats
      avgMessagesPerChat
      __typename
    }
    priorWeekUsage {
      usersWithUsage
      percentUsersWithUsage
      avgChats
      avgMessagesPerChat
      __typename
    }
    top5Activities {
      activityType
      aIGuideActivity {
        id
        translatedTitle
        __typename
      }
      aiReportingLabel {
        id
        translatedTitle
        __typename
      }
      userPercentage
      chatPercent
      totalChats
      totalUsers
      __typename
    }
    target {
      percentage
      __typename
    }
    __typename
  }
}