query SkillsQuery($classDescriptor: String!, $skillIds: [String]!, $studentKaid: String) {
  teacherOverallProgressReport(classroomDescriptor: $classDescriptor, filters: {studentKaid: $studentKaid}) {
    skillsLevels(skillIds: $skillIds) {
      skillId
      studentLevels {
        skillLevel
        studentKaid
        __typename
      }
      __typename
    }
    __typename
  }
}
