query guideTranscripts($kaid: String!, $cursor: String, $startDate: DateTime, $endDate: DateTime, $moderationStatus: ModerationStatus) {
  aiGuideThreads(
    kaid: $kaid
    cursor: $cursor
    startDate: $startDate
    endDate: $endDate
    moderationStatus: $moderationStatus
  ) {
    threads {
      id
      lastUpdatedAt
      flagged
      title
      interactions {
        id
        pageType
        pageTitle
        question
        answer
        __typename
      }
      coeditingDocument {
        id
        title
        __typename
      }
      __typename
    }
    cursor
    __typename
  }
}