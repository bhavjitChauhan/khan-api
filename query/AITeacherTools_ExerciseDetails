query AITeacherTools_ExerciseDetails($exerciseId: String) {
  exerciseById(id: $exerciseId) {
    id
    title
    prerequisites
    parentTopic {
      id
      domainSlug
      __typename
    }
    assessmentItems {
      id
      itemShapeType
      itemData
      problemType
      __typename
    }
    __typename
  }
}