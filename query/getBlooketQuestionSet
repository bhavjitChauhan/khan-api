query getBlooketQuestionSet($threadId: String!) {
  blooketQuestionSet(threadId: $threadId) {
    id
    ownerKaid
    threadId
    title
    description
    questions {
      text
      timeLimit
      randomizeAnswers
      answers {
        text
        isCorrect
        __typename
      }
      __typename
    }
    __typename
  }
}