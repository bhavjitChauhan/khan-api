query guideTranscript($transcriptId: String!) {
  aiGuideThread(threadId: $transcriptId) {
    id
    summary
    lastUpdatedAt
    flagged
    insights {
      interests
      topics
      safety
      confusing
      understood
      celebrate
      __typename
    }
    interactions {
      id
      answer
      question
      url
      createdAt
      flagged
      flaggedCategories
      responseWasStoppedByUser
      __typename
    }
    __typename
  }
}
