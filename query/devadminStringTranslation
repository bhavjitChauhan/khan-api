query devadminStringTranslation($message: String!, $messageLocation: String!, $messageContext: String, $kaLocale: KALocale!) {
  stringTranslation(
    message: $message
    messageLocation: $messageLocation
    messageContext: $messageContext
    kaLocale: $kaLocale
  ) {
    found
    isAITranslated
    isNewTranslation
    translation
    __typename
  }
}