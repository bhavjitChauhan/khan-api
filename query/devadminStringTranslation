query devadminStringTranslation($message: String!, $messageContext: String, $kaLocale: KALocale!) {
  stringTranslation(
    message: $message
    messageContext: $messageContext
    kaLocale: $kaLocale
    messageLocation: "devadmin-automatic-translation-test"
  ) {
    found
    isAITranslated
    isNewTranslation
    translation
    __typename
  }
}