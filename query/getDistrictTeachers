query getDistrictTeachers($districtId: ID!, $pageSize: Int, $cursor: Int) {
  activationReportByTeacher: activationReportByTeacherV2(
    districtId: $districtId
    pageSize: $pageSize
    cursor: $cursor
  ) {
    rows {
      isActivated
      teacherKaid
      teacherName
      __typename
    }
    __typename
  }
}