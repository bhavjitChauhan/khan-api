query getModerationLogs($limit: Int!, $cursor: String!, $beforeTimestampSeconds: String, $excludeModeratorKaids: [String!]) {
  moderationLogs(
    limit: $limit
    cursor: $cursor
    beforeTimestampSeconds: $beforeTimestampSeconds
    excludeModeratorKaids: $excludeModeratorKaids
  ) {
    moderationLogs {
      date
      description {
        href
        linkText
        postText
        preText
        __typename
      }
      moderator {
        id
        kaid
        nickname
        __typename
      }
      __typename
    }
    nextCursor
    __typename
  }
}