query districtLastNKhanmigoJobs($districtId: ID!, $includedDryRun: Boolean!, $numJobs: Int!) {
  districtById(districtId: $districtId) {
    id
    lastNKhanmigoJobs(includeDryRun: $includedDryRun, numJobs: $numJobs) {
      createdAt
      rosterSource
      timeSpentSeconds
      dryRunStatus
      numStudents
      numNewStudents
      numRemovedStudents
      numTeachers
      numNewTeachers
      numRemovedTeachers
      khanmigoTotal
      khanmigoAddedTotal
      khanmigoRemovedTotal
      __typename
    }
    __typename
  }
}
