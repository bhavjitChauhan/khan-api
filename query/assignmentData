query assignmentData($assignmentID: String!) {
  user {
    id
    assignment(id: $assignmentID) {
      id
      title
      itemCompletionStates {
        studentKaid
        completedOn
        activitySubmissions {
          threadID
          __typename
        }
        __typename
      }
      classroom {
        id
        cacheId
        name
        __typename
      }
      __typename
    }
    __typename
  }
}