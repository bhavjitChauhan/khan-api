query getClassesAndStudents {
  coach: user {
    id
    studentLists: coachedStudentLists {
      id
      descriptor
      cacheId
      name
      signupCode
      studentKaidsAndNicknames {
        id
        kaid
        coachNickname
        __typename
      }
      topics {
        id
        __typename
      }
      __typename
    }
    __typename
  }
}