query districtGrowthBySchool($filters: DistrictGrowthFilters!) {
  districtGrowthBySchool(filters: $filters) {
    school {
      id
      name
      __typename
    }
    info {
      numStudents
      medianCMPercent
      wowPercentChange
      eoyPercentGuess
      __typename
    }
    __typename
  }
}