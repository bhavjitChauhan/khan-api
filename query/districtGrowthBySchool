query districtGrowthBySchool($filters: DistrictGrowthFilters!) {
  districtGrowthBySchool(filters: $filters) {
    school {
      id
      name
      __typename
    }
    info {
      numStudents
      medianCMPercent
      wowPercentChange
      eoyPercentGuess
      course {
        id
        __typename
      }
      targetStatus
      __typename
    }
    target {
      course {
        id
        __typename
      }
      percentage
      gradeLevels
      __typename
    }
    __typename
  }
}