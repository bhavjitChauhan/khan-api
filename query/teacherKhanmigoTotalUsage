query teacherKhanmigoTotalUsage($filter: KhanmigoTeacherFilters!) {
  teacherKhanmigoUsage(filter: $filter) {
    byStudent {
      student {
        id
        kaid
        coachNickname
        __typename
      }
      chats
      messages
      avgMessagesPerChat
      activities
      __typename
    }
    __typename
  }
}