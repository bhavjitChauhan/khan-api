query gtp_getExamGroupMetadata($examGroupId: String!) {
  examGroup(examGroupId: $examGroupId) {
    id
    examGroupMetadata {
      ...gtp_examGroupMetadataFragment
      __typename
    }
    __typename
  }
}

fragment gtp_examGroupMetadataFragment on ExamGroupMetadata {
  id
  testMaker
  kaRelationshipToTestCopy
  testMakerLogoImg
  registrationUrl
  __typename
}