query getChildrenForDevadmin($kaid: String!) {
  user(kaid: $kaid) {
    id
    maybeManagedChildren {
      id
      canModifyCoaches
      email
      isUnderAgeGate: isChild
      isSpawnedByCurrentUser: isSpawnedBy(coachKaid: $kaid)
      kaid
      nickname
      username
      grassrootsKhanmigoIneligibleReasons
      __typename
    }
    unmanagedChildren {
      id
      canModifyCoaches
      email
      isUnderAgeGate: isChild
      isSpawnedByCurrentUser: isSpawnedBy(coachKaid: $kaid)
      kaid
      nickname
      username
      grassrootsKhanmigoIneligibleReasons
      __typename
    }
    __typename
  }
}