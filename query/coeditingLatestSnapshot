query coeditingLatestSnapshot($threadId: String!) {
  aiGuideThread(threadId: $threadId) {
    id
    coeditingDocument {
      id
      title
      latestSnapshot {
        id
        text {
          prefix
          selection
          suffix
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}