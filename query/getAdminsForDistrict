query getAdminsForDistrict($districtID: String!) {
  getAdminsForDistrict(districtID: $districtID) {
    id
    uuid
    displayNameForTeacher
    districtProvidedEmail
    kaid
    moderatorOfSchools {
      id
      __typename
    }
    adminOfSchools {
      id
      __typename
    }
    __typename
  }
}