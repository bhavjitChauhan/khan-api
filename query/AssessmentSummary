query AssessmentSummary($slug: String!) {
  user {
    id
    assessmentData {
      assessmentStatus(assessmentSlug: $slug) {
        assessment {
          id
          surveySlug
          __typename
        }
        summary {
          questionsCorrect
          questionsTotal
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}