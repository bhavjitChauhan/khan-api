query essaySession($id: String!) {
  essaySession(essaySessionID: $id) {
    draft {
      feedbackList {
        dimension
        feedbackID
        feedbackText
        _: id
        isPositive
        isResolved
        relevantParagraphId
        relevantQuotedText
        thread {
          id
          interactions {
            annotations
            answer
            flagged
            id
            question
            __typename
          }
          __typename
        }
        __typename
      }
      id
      submittedText
      __typename
    }
    essayInstructions
    essayTitle
    essayVersion
    id
    mostRecentEditedText
    paragraphIDToParagraphNumbers {
      paragraphID
      paragraphNumbers
      __typename
    }
    studentGradeLevel
    __typename
  }
}