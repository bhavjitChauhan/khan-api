query courseEditorGetEditingIdentity($descriptors: [String!]!) {
  revisionsForDescriptors(descriptors: $descriptors) {
    ... on ContentRevision {
      id
      sha
      __typename
    }
    ... on LearnableContentRevision {
      doNotPublish
      slug
      sourceKaLocale
      importedFromSha
      listed
      title
      __typename
    }
    ... on ExerciseRevision {
      interimAssessmentType
      suggestedCompletionCriteria
      __typename
    }
    __typename
  }
}