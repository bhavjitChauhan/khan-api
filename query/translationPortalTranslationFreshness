query translationPortalTranslationFreshness {
  platformTranslationProgress {
    ...tapTranslationFreshness
    __typename
  }
}

fragment tapTranslationFreshness on TAPTranslationFreshness {
  lastStringsUpdateDate
  lastDubsUpdateDate
  lastSubtitlesUpdateDate
  oldestSubtitlesDate
  updateDate
  __typename
}