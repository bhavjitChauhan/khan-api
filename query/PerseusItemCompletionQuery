query PerseusItemCompletionQuery($exerciseDetails: ExerciseDetailsInput!, $templateDetails: PerseusTemplateInput!, $numItems: Int!, $additionalInstructions: String) {
  perseusItemCompletion(
    exerciseDetails: $exerciseDetails
    templateDetails: $templateDetails
    numItems: $numItems
    additionalInstructions: $additionalInstructions
  ) {
    items
    error {
      code
      debugMessage
      __typename
    }
    __typename
  }
}