query DistrictAdminPermissionsQuery {
  user {
    id
    hasPermission(name: "CAN_ACT_LIKE_A_DISTRICT_ADMIN", scope: ANY)
    userDistrictInfos {
      id
      district {
        id
        __typename
      }
      activatedAt
      canSeeAllSchools
      canManageAdmins
      isKmap
      isSuperAdmin
      __typename
    }
    __typename
  }
}