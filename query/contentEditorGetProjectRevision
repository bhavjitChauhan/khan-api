query contentEditorGetProjectRevision($id: String!) {
  projectRevisionById(id: $id) {
    ...ProjectRevision
    __typename
  }
}

fragment ProjectRevision on ProjectRevision {
  id
  contentId
  contentKind
  sha
  doNotPublish
  slug
  listed
  title
  description
  projectEval
  code
  codeFormat
  __typename
}