query aiGuidePrompt($name: String!) {
  aiGuidePrompt(name: $name) {
    id
    name
    description
    questions {
      id
      questionText
      __typename
    }
    versions {
      id
      versionNumber
      releaseNotes
      promptText
      configJSON
      tokenCount
      __typename
    }
    draft {
      hash
      isReadyForReview
      releaseNotes
      promptText
      configJSON
      tokenCount
      __typename
    }
    source
    __typename
  }
}