query SatPracticeByGradeQuery($districtName: String!, $schoolIds: [ID!], $gradeLevels: [DistrictGradeLevel!], $progressFrom: DateTime, $progressUpTo: DateTime) {
  districtSatPracticeReportByGrade(districtName: $districtName, filters: {schoolIds: $schoolIds, gradeLevels: $gradeLevels, progressFrom: $progressFrom, progressUpTo: $progressUpTo}) {
    avgRow {
      avgPracticeMinutes
      avgMathPractice
      avgReadingAndWritingPractice
      __typename
    }
    rows {
      gradeLevel {
        id
        name
        sortIndex
        __typename
      }
      practiceMinutes
      mathPractice
      readingAndWritingPractice
      __typename
    }
    __typename
  }
}
