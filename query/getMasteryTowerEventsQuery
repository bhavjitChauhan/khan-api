query getMasteryTowerEventsQuery($classDescriptor: String!) {
  teacherMasteryTowerCurrentEvents(classroomDescriptor: $classDescriptor) {
    events {
      id
      eventTime
      pointsEarned
      source
      student {
        id
        kaid
        nickname
        __typename
      }
      exercise {
        id
        title
        defaultUrlPath
        __typename
      }
      __typename
    }
    __typename
  }
}