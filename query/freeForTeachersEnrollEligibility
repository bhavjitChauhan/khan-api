query freeForTeachersEnrollEligibility($agreesToTerms: Boolean) {
  user {
    id
    aiGuideAccessPlans {
      id
      enrollmentGroup
      isActive
      __typename
    }
    birthdate
    email
    username
    aiGuideFreeForTeachersEligibility(agreesToAIGuideTermsOfService: $agreesToTerms) {
      age
      aiGuideTermsOfService
      isTeacher
      kadStudent
      __typename
    }
    __typename
  }
}