query perseusTemplateCompletionQuery($exemplar: String!, $exerciseTitle: String!, $exerciseDescription: String!, $standardsDescriptions: String!, $additionalInstructions: String) {
  perseusTemplateCompletion(
    exemplar: $exemplar
    exerciseDetails: {title: $exerciseTitle, description: $exerciseDescription, standardsDescriptions: $standardsDescriptions}
    additionalInstructions: $additionalInstructions
  ) {
    completion {
      template
      misconceptions
      placeholderNames
      __typename
    }
    error {
      code
      __typename
    }
    __typename
  }
}