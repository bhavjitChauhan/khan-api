query getCoach {
  coach: user {
    id
    nickname
    __typename
  }
  user {
    id
    tosForFormalTeacherStatus
    includesCentrallyOwnedDistrictData
    userDistrictInfos {
      id
      rosterSource
      district {
        id
        name
        __typename
      }
      __typename
    }
    schoolAffiliation {
      id
      name
      country
      postalCode
      administrativeAreaLevel1
      administrativeAreaLevel2
      locality
      __typename
    }
    affiliationCountryCode
    __typename
  }
}