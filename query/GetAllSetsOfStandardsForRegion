query GetAllSetsOfStandardsForRegion($isDiscoverable: Boolean, $regionSlug: String!) {
  standardRegions(isDiscoverable: $isDiscoverable, slug: $regionSlug) {
    error {
      code
      debugMessage
      __typename
    }
    standardRegion {
      id
      imageUrl
      isDiscoverable
      __typename
    }
    __typename
  }
  allSetsOfStandards(
    domain: "math"
    isDiscoverable: $isDiscoverable
    regionSlug: $regionSlug
  ) {
    id
    name
    courseEntities {
      id
      relativeUrl
      standaloneTitle
      __typename
    }
    standards {
      id
      setId
      standardId
      description
      children
      relativeUrl
      mappedContent {
        ... on LearnableContent {
          id
          contentKind
          title
          defaultUrlPath
          __typename
        }
        __typename
      }
      __typename
    }
    domains
    __typename
  }
}