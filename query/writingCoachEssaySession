query writingCoachEssaySession($id: String!) {
  essaySession(essaySessionID: $id) {
    assignmentID
    draft {
      feedbackList {
        dimension
        feedbackID
        feedbackText
        _USE_feedbackID_INSTEAD_: id
        isPositive
        isResolved
        relevantParagraphId
        relevantQuotedText
        thread {
          id
          interactions {
            annotations
            answer
            flagged
            id
            question
            __typename
          }
          __typename
        }
        __typename
      }
      id
      submittedText
      __typename
    }
    completed
    draftingThreadID
    essayInstructions
    essayTitle
    essayType
    essayVersion
    gettingStartedThreadID
    id
    mostRecentEditedText
    minimumWordCount
    outline
    outlineVersion
    outliningThreadID
    paragraphIDToParagraphNumbers {
      paragraphID
      paragraphNumbers
      __typename
    }
    studentGradeLevel
    __typename
  }
}