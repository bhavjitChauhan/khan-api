query writingCoachEssaySession($id: String!) {
  essaySession(essaySessionID: $id) {
    draft {
      id
      __typename
    }
    draftingThreadID
    essayInstructions
    essayTitle
    essayType
    gettingStartedThreadID
    id
    minimumWordCount
    outliningThreadID
    studentGradeLevel
    __typename
  }
}