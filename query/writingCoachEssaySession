query writingCoachEssaySession($id: String!) {
  essaySession(essaySessionID: $id) {
    draft {
      id
      __typename
    }
    draftingThreadID
    essayInstructions
    essayTitle
    essayType
    essayVersion
    gettingStartedThreadID
    id
    mostRecentEditedText
    minimumWordCount
    outline
    outlineVersion
    outliningThreadID
    studentGradeLevel
    __typename
  }
}