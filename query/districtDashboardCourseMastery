query districtDashboardCourseMastery($filters: DistrictGrowthFilters!) {
  districtDashboardCourseMastery(filters: $filters) {
    header {
      yearFirstWeekStart
      yearFirstWeekEnd
      yearLastWeekStart
      yearLastWeekEnd
      numberOfWeeksInTheSchoolYear
      lastWeekAvg
      yearToDateAvg
      noData
      __typename
    }
    rows {
      weekStartDate
      weekEndDate
      overallMedianCMPercent
      selectedSchoolsMedianCMPercent
      targetPercentage
      __typename
    }
    __typename
  }
}