query getAdminsForInProductAdminRostering($districtID: String!) {
  getAdminsForDistrict(districtID: $districtID, allowPartial: true) {
    id
    displayNameForTeacher
    districtProvidedEmail
    districtProvidedFirstName
    districtProvidedLastName
    primaryRole
    canManageAdmins
    canSeeAllSchools
    activationEmailSentAt
    rosterSource
    cleverId
    classlinkId
    kaid
    adminOfSchools(onlyVisible: true) {
      id
      name
      __typename
    }
    hasAdminDirectedInfo
    __typename
  }
}