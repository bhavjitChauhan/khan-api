query getAdminsForInProductAdminRostering($partnershipID: ID!, $districtID: ID!) {
  getVisibleAdminUDIs(
    partnershipID: $partnershipID
    districtID: $districtID
    allowPartial: true
  ) {
    id
    displayNameForTeacher
    districtProvidedEmail
    districtProvidedFirstName
    districtProvidedLastName
    primaryRole
    canManageAdmins
    canSeeAllSchools
    activationEmailSentAt
    rosterSource
    cleverId
    classlinkId
    kaid
    adminOfSchools(onlyVisible: true) {
      id
      name
      __typename
    }
    hasAdminDirectedInfo
    __typename
  }
  getVisibleUMIs(partnershipID: $partnershipID) {
    id
    email
    firstName
    lastName
    displayName
    canManageAdmins
    activationEmailSentAt
    kaid
    adminOf {
      ... on MetaDistrict {
        id
        name
        __typename
      }
      ... on District {
        id
        name
        __typename
      }
      __typename
    }
    adminOfHeight
    __typename
  }
}