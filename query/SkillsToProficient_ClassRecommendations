query SkillsToProficient_ClassRecommendations($courseId: String, $classroomDescriptor: String) {
  user {
    id
    recommendations(
      queryParams: {courseId: $courseId, classroomDescriptor: $classroomDescriptor}
    ) {
      content {
        id
        defaultUrlPath
        __typename
      }
      __typename
    }
    __typename
  }
}