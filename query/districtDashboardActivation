query districtDashboardActivation($filters: DistrictDashboardFilters!) {
  districtDashboardActivation(filters: $filters) {
    numRosteredStudents
    numRosteredTeachers
    numTeachersWithAnAccount
    percentTeachersWithAnAccount
    numWithAnAccount
    percentWithAnAccount
    numStartedAnActivity
    percentStartedAnActivity
    lastUpdatedDate
    target {
      percentage
      __typename
    }
    __typename
  }
}