query districtTeachers($districtName: String!, $pageSize: Int, $cursor: Int) {
  activationReportByTeacher: activationReportByTeacherV2(
    districtName: $districtName
    pageSize: $pageSize
    cursor: $cursor
  ) {
    rows {
      teacherId
      teacherName
      __typename
    }
    __typename
  }
}