query aiGuideInvitationInfo($token: String!) {
  aiGuideInvitationInfo(token: $token) {
    digitalTermsOfServiceURL
    isExpired
    hasHitUsageLimit
    __typename
  }
  isEnrolledInAIGuide
  user {
    id
    age
    aiGuideInvitationAcceptance(token: $token) {
      acceptedAt
      __typename
    }
    __typename
  }
}