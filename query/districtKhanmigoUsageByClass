query districtKhanmigoUsageByClass($filter: KhanmigoDistrictFilters!, $schoolID: String!) {
  districtKhanmigoUsage(filter: $filter) {
    byClass(schoolID: $schoolID) {
      classroom {
        id
        cacheId
        name
        coachKaid
        descriptor
        __typename
      }
      usage {
        possibleUsers
        usersWithUsage
        percentUsersWithUsage
        chats
        avgChatsPerUsersWithUsage
        avgChatsPerPossibleUsers
        flaggedChats
        avgFlaggedChatsPerUsersWithUsage
        avgFlaggedChatsPerPossibleUsers
        messages
        avgMessagesPerChat
        targetStatus
        avgMessagesPerUserWithUsage
        avgMessagesPerPossibleUsers
        __typename
      }
      __typename
    }
    __typename
  }
}