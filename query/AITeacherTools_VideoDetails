query AITeacherTools_VideoDetails($videoId: String) {
  videoById(contentId: $videoId) {
    id
    translatedTitle
    defaultUrlPath
    parentTopic {
      id
      domainSlug
      allLearnableContent(includeUnlisted: false) {
        id
        translatedTitle
        defaultUrlPath
        __typename
      }
      __typename
    }
    subtitles {
      text
      __typename
    }
    __typename
  }
}