query courseEditorGetPublishedVideo($contentId: String!) {
  videoById(contentId: $contentId) {
    ...LearnableContent
    __typename
  }
}
fragment LearnableContent on LearnableContent {
  id
  kind
  creationDate
  topicPaths {
    path {
      id
      kind
      slug
      __typename
    }
    __typename
  }
  __typename
}