query GetSetOfStandards($setId: String!, $region: String) {
  setOfStandards(setId: $setId, region: $region) {
    id
    isDiscoverable
    name
    standardRegionIds
    domains
    standards {
      id
      standardId
      description
      coverage
      children
      groupLabel
      mappedContent {
        ... on LearnableContent {
          id
          contentKind
          title
          urlWithinStandardSet(setId: $setId)
          defaultUrlPath
          __typename
        }
        ... on Exercise {
          interimAssessmentType
          __typename
        }
        __typename
      }
      __typename
    }
    __typename
  }
}