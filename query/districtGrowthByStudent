query districtGrowthByStudent($classroomDescriptor: String!, $filters: DistrictGrowthFilters!) {
  districtGrowthByStudent(
    classroomDescriptor: $classroomDescriptor
    filters: $filters
  ) {
    user {
      id
      kaid
      userDistrictInfos {
        id
        displayNameForTeacher
        __typename
      }
      __typename
    }
    info {
      numStudents
      medianCMPercent
      wowPercentChange
      eoyPercentGuess
      targetStatus
      __typename
    }
    __typename
  }
}