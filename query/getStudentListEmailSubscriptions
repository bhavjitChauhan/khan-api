query getStudentListEmailSubscriptions($classroomDescriptor: String!) {
  coach: user {
    id
    emailSubscriptions {
      coachReportSubscription: classroomEmailSubscriptionForClassroomDescriptor(
        classroomDescriptor: $classroomDescriptor
      ) {
        checked
        token
        __typename
      }
      __typename
    }
    __typename
  }
}