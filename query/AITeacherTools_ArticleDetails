query AITeacherTools_ArticleDetails($articleId: String) {
  articleById(id: $articleId) {
    id
    translatedTitle
    defaultUrlPath
    translatedPerseusContent
    parentTopic {
      id
      domainSlug
      allLearnableContent(includeUnlisted: false) {
        id
        translatedTitle
        defaultUrlPath
        __typename
      }
      __typename
    }
    __typename
  }
}