    title
    kind
    __typename
  }
  studentList {
    id
    cacheId
    name
    __typename
  }
  students {
    id
    kaid
    __typename
  }
  coach {
    id
    kaid
    __typename
  }
  startDate
  dueDate
  isDraft
  subjectSlug
  title
  instructions
  __typename
}