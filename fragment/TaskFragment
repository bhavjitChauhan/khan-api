fragment TaskFragment on AssessmentTask {
  id
  expirationTime
  questionsCompleted
  estimatedQuestionsRemaining
  currentStep {
    stepNumber
    stepId
    type
    assessmentItem {
      id
      exerciseId
      itemData
      __typename
    }
    ... on EYTQuestion {
      conversationStarter
      completionCriteria
      __typename
    }
    __typename
  }
  __typename
}